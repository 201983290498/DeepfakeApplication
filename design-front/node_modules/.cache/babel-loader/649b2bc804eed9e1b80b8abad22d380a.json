Box: 'example1'\r\n    }\r\n  },\r\n  props: {\r\n    disImgs: []\r\n  },\r\n  methods: {\r\n    changeDisboard: function (event) {\r\n      const _this = this\r\n      $('.example img').removeClass('wrap-mask')\r\n      $('.example img').addClass('wrap-mask')\r\n      $(event.target).removeClass('wrap-mask')\r\n      $('.display-main').removeClass(_this.activeBox)\r\n      _this.activeBox = $(event.target).parent().attr('id')\r\n      $('.display-main').addClass(_this.activeBox)\r\n    }\r\n  },\r\n  created () {\r\n    const _this = this\r\n    console.log(_this.disImgs[0])\r\n  },\r\n  mounted () {\r\n    const _this = this\r\n    // todo jQuery 添加css\r\n    $('.example img').addClass('wrap-mask')\r\n    $('.example1 img').removeClass('wrap-mask')\r\n    $('.display-main').addClass(_this.activeBox)\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.service-display{\r\n  padding: 10px 0;\r\n}\r\n.inb{\r\n  display: inline-block\r\n}\r\n.display-main{\r\n  width: 70%;\r\n  height: 430px;\r\n  margin-right: 10px;\r\n  display: inline-block;\r\n  margin-bottom: 20px;\r\n  border-radius: 40px;\r\n}\r\n.display-minor{\r\n  float: right;\r\n  display: inline-block;\r\n  width: 25%;\r\n}\r\n.example {\r\n  height: 140px;\r\n  margin-bottom: 10px;\r\n  border-radius: 16px !important;\r\n}\r\n.example1 {\r\n  background-image: url(\"../../static/imgs/fake1.jpg\");\r\n  background-size: contain;\r\n  background-repeat: no-repeat;\r\n  background-color: #211004;\r\n  background-position: center;\r\n}\r\n.example2 {\r\n  background-image: url(\"../../static/imgs/fake2.jpg\");\r\n  background-size: contain;\r\n  background-repeat: no-repeat;\r\n  background-color: #211004;\r\n  background-position: center;\r\n}\r\n.example3 {\r\n  background-image: url(\"../../static/imgs/real1.jpg\");\r\n  background-size: contain;\r\n  background-repeat: no-repeat;\r\n  background-color: #211004;\r\n  background-position: center;\r\n}\r\n.wrap-mask{\r\n  height:100%;\r\n  width:100%;\r\n  border-radius: 16px;\r\n  background: rgba(0,0,0,.4);\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}